{
  "framework": "vite",
  "buildCommand": "node scripts/fix-rollup-vercel.cjs && node scripts/rollup-fix-builder.cjs",
  "outputDirectory": "dist",
  "rewrites": [
    { "source": "/(.*)", "destination": "/index.html" }
  ],
  "env": {
    "ROLLUP_NATIVE_DISABLE": "1",
    "NODE_OPTIONS": "--max-old-space-size=4096"
  }
}